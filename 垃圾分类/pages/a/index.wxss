.one{
  width: 100%;
  /* height: 800px; */
  background: rgb(236, 235, 235);
}
.a_left{
  width: 65px;
  float: left;
  position: fixed;
  top: 10px;
}
.a_one,.a_two,.a_three,.a_four{
  width: 65px;
  height: 45px;
  text-align: center;
  line-height: 45px;
  font-size: 7px;
  color: #fff;
  background: #ccc;
  transition: 0.5s;
}
.active1{
  
    background: #956b58;
}
.active2{
    background: #2d2926;
}
.active3{
  background: #003d78;
}
.active4{
  background: #fd4138;
}
.a_right{
  float: right;
  width: 236px;
}
.a_rights{
border-radius: 20px;
margin-top: 30px;
padding: 10px 15px;
background: #f4f0ed;
}
image{
  width: 64px;
  height: 64px;
  display: block;
  margin: 10px auto;
}
.fontWit{
  font-size: 10px;
  font-weight: bold;
  color: #000;
}
text{
  font-size: 12px;
  color: #7d7a78;
}
.a_views rich-text{
  display: inline-block;
  width: 100%;
  height: 40px;
  font-size: 13px;
  line-height: 40px;
  padding-left: 20px;
}
.a_views rich-text:nth-child(even){
  background: #fafafa;
}
.show{
  display: block;
}
.hidden{
  display: none;
}