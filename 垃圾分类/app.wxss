/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  /* position: relative; */
} 
.max{
  width: 100%;
  position: fixed;
  bottom: 0px;
  display: flex;
  background: #fff;
  height: 40px;
  line-height: 50px;
  font-size: 9px;
}
.max navigator{
  flex: 1;
  text-align: center;
  z-index: 100;
  border-left: 1px solid #000;
  border-right: 1px solid #000;
  margin: 0 5px; 
}
.max navigator:first-child{
  border-left:0; 
}
